{
  "name": "vagas",
  "version": "1.0.0",
  "description": "Sistema para controle de vagas",
  "main": "index.js",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "ts-node-dev -r tsconfig-paths/register --poll --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register --poll ./node_modules/typeorm/cli",
    "typeorm:vagas": "ts-node-dev -r tsconfig-paths/register --poll ./node_modules/typeorm/cli -f ormconfig.vagas.json",
    "typeorm:noticias": "ts-node-dev -r tsconfig-paths/register --poll ./node_modules/typeorm/cli -f ormconfig.noticias.json",
    "typeorm:common": "ts-node-dev -r tsconfig-paths/register --poll ./node_modules/typeorm/cli -f ormconfig.common.json",
    "migration:run:vagas": "npm run typeorm:vagas migration:run",
    "migration:run:noticias": "npm run typeorm:noticias migration:run",
    "migration:run:common": "npm run typeorm:common migration:run",
    "migration:run:all": "npm run migration:run:common && npm run migration:run:vagas && npm run migration:run:noticias",
    "migration:revert:vagas": "npm run typeorm:vagas migration:revert",
    "migration:revert:noticias": "npm run typeorm:noticias migration:revert",
    "migration:revert:common": "npm run typeorm:common migration:revert",
    "migration:create:vagas": "npm run typeorm:vagas migration:create -n",
    "migration:create:noticias": "npm run typeorm:noticias migration:create -n",
    "migration:create:common": "npm run typeorm:common migration:create -n",
    "migration:show:vagas": "npm run typeorm:vagas migration:show",
    "migration:show:noticias": "npm run typeorm:noticias migration:show",
    "migration:show:common": "npm run typeorm:common migration:show",
    "setup:db": "ts-node-dev -r tsconfig-paths/register scripts/setup-database.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "seed:admin": "ts-node-dev -r tsconfig-paths/register src/shared/infra/typeorm/seed/admin.ts",
    "seed:areas": "ts-node-dev -r tsconfig-paths/register src/shared/infra/typeorm/seed/areasAtuacao.ts",
    "seed:associados-recrutadores": "ts-node-dev -r tsconfig-paths/register src/shared/infra/typeorm/seed/associados-recrutadores.ts",
    "reset:admin-password": "ts-node-dev -r tsconfig-paths/register src/shared/infra/typeorm/seed/reset-admin-password.ts"
  },
  "author": "Ras Technology",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^7.80.1",
    "@sentry/profiling-node": "^1.2.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "aws-sdk": "^2.1418.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "csv-parse": "^4.16.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mime": "2.5.2",
    "multer": "^1.4.3",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.1",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "ts-jest": "^29.0.5",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.2.43",
    "uuid": "^8.3.2",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.15",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-typescript": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.4.3",
    "prettier": "^3.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.3.3"
  }
}
